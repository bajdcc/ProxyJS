{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
	"static"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": ""
  },
  "watch": [
    "server.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}